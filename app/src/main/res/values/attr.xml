<?xml version="1.0" encoding="utf-8"?>
<resources>

   <!--flowlayout 首页分类，或搜索历史-->
    <declare-styleable name="FlowLayout">
        <attr name="lineSpacing" format="dimension" />
        <attr name="maxLine" format="integer" />
    </declare-styleable>


    <declare-styleable name="MaterialLayout">
        <attr name="alpha_1" format="integer" />
        <attr name="alpha_step" format="integer" />
        <attr name="framerate" format="integer" />
        <attr name="duration" format="integer" />
        <attr name="color" format="color" />
        <attr name="scale" format="float" />
    </declare-styleable>


    <!--TabIndicator的参数设置-->
    <declare-styleable name="PagerSlidingTab">
        <attr name="indicatorColor" format="color" />
        <attr name="tabTextColorNoSelect" format="color" />
        <attr name="tabTextColorSelected" format="color" />
        <attr name="underlineColor" format="color" />
        <attr name="dividerColor" format="color" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="underlineHeight" format="dimension" />
        <attr name="pst_dividerPadding" format="dimension" />
        <attr name="tabPaddingLeftRight" format="dimension" />
        <attr name="scrollOffset" format="dimension" />
        <attr name="tabWidth" format="dimension" />
        <attr name="tabBackgrounds" format="reference" />
        <attr name="sameLine" format="boolean" />
        <attr name="pst_textAllCaps" format="boolean" />
    </declare-styleable>

</resources>